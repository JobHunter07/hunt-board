{
  "name": "@jobhunter07/hunt-board",
  "version": "0.1.0",
  "description": "Hunt Board NPM Library - Kanban-style job hunting dashboard with Material UI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "kanban",
    "job-hunting",
    "material-ui",
    "react",
    "typescript",
    "drag-and-drop"
  ],
  "author": "JobHunter07",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^6.19.5",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.71.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.3.0",
    "@hookform/resolvers": "^3.10.0",
    "@playwright/experimental-ct-react": "^1.58.2",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-links": "^10.2.10",
    "@storybook/jest": "^0.2.2",
    "@storybook/react": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "babel-plugin-import": "^1.13.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^10.2.10",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^10.2.10",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vitest": "^1.3.1"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/JobHunter07/hunt-board.git"
  },
  "bugs": {
    "url": "https://github.com/JobHunter07/hunt-board/issues"
  },
  "homepage": "https://github.com/JobHunter07/hunt-board#readme"
}
